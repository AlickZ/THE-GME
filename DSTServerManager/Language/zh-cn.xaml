    <s:String x:Key="ModInfo_Auth">作者</s:String>
    <s:String x:Key="ModInfo_Vern">版本</s:String>
    <s:String x:Key="ModInfo_ApiV">API</s:String>
    <s:String x:Key="ModInfo_Muti">联机</s:String>
    <s:String x:Key="ModInfo_Solo">单机</s:String>
    <s:String x:Key="ModInfo_Dlc1">巨人</s:String>
    <s:String x:Key="ModInfo_allC">客户端需求</s:String>

    <!--================================================================================-->
    <!--本地服务器 开启方式-->
    <s:String x:Key="LocalServer_OpenType">本地服务器开启方式</s:String>
    <s:String x:Key="LocalServer_OpenType_1">普通窗口   *以Windows标准Shell窗口开启服务器</s:String>
    <s:String x:Key="LocalServer_OpenType_2">内置窗口   *以程序内置标签页窗口开启服务器</s:String>

    <!--本地服务器 服务器列表-->
    <s:String x:Key="LocalServer_ServerList">本地服务器列表</s:String>
    <s:String x:Key="LocalServer_ServerList_Index">编号</s:String>
    <s:String x:Key="LocalServer_ServerList_Name">类型</s:String>
    <s:String x:Key="LocalServer_ServerList_Path">路径</s:String>
    <s:String x:Key="LocalServer_AddServer">添加服务器</s:String>
    <s:String x:Key="LocalServer_GetServer">获取服务器</s:String>
    <s:String x:Key="LocalServer_DelServer">删除服务器</s:String>

    <!--本地服务器 集群存档文件列表-->
    <s:String x:Key="LocalServer_ClusterSaveList">本地服务器集群存档文件列表</s:String>

    <!--远程服务器 远程连接列表-->
    <s:String x:Key="CloudServer_ConnectionList">远程连接列表</s:String>


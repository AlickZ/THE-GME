<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:DSTServerManager"
                    xmlns:s="clr-namespace:System;assembly=mscorlib">
    <!--================================================================================-->
    <!--子窗口-->
    <s:String x:Key="SubWin_ConnectionCheck">测试连接</s:String>
    <s:String x:Key="SubWin_ConnectionSaved">保存连接</s:String>

    <!--================================================================================-->
    <!--程序菜单项目-->
    <s:String x:Key="MenuItem_ApplicationMenu">菜单</s:String>
    <s:String x:Key="ApplicationMenu_Close">退出</s:String>

    <s:String x:Key="MenuItem_ServerCtrlsMenu">服务器</s:String>
    <s:String x:Key="ServerCtrlsMenu_Nothing">暂无</s:String>

    <s:String x:Key="MenuItem_SaveControlMenu">存档</s:String>
    <s:String x:Key="SaveControlMenu_Nothing">暂无</s:String>

    <s:String x:Key="MenuItem_SelectLanguages">语言</s:String>
    <s:String x:Key="SelectLanguages_Chinese">中文</s:String>
    <s:String x:Key="SelectLanguages_English">English</s:String>

    <!--================================================================================-->
    <!--右侧标签页-->
    <s:String x:Key="GROUP_CLUSTER_SETTING">服务器集群设置</s:String>
    <s:String x:Key="GROUP_CLUSTER_SERVERS">集群服务器列表</s:String>
    <s:String x:Key="GROUP_CLUSTER_SERINFO">服务器信息列表</s:String>

    <s:String x:Key="GAMEPLAY_GMMODE">游戏模式</s:String>
    <s:String x:Key="GAMEPLAY_COUNTS">玩家数量</s:String>
